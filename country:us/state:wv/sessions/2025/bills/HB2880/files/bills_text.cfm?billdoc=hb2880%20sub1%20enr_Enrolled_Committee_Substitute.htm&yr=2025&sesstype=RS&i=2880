		.cs8DF0654B{text-align:justify;text-indent:0pt;margin:0pt 36pt 0pt 0pt}
		.cs73AF651A{text-align:justify;text-indent:0pt;margin:0pt 36pt 0pt 36pt}
		.csF59181DF{text-align:right;text-indent:0pt;margin:0pt 36pt 0pt 36pt}
